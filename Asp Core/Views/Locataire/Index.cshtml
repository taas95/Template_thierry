@* @model TModel *@
@model Immeuble
@{
    Layout = "_Layout";
}

<partial name="_SideMenu"/>

<section class="container margin-top">

    <div class="row">
        <div class="col s12 l2">
            <a href="" data-target="slide-out" data-position="bottom" data-delay="50" data-tooltip="Montrer le menu"
               class="waves-effect waves-light sidenav-trigger tooltipped hide-on-med-and-down">
                <i class="material-icons">menu</i>
            </a>
        </div>
        <div class="col s12 l8">


            <form action="">
                <div class="row"/>
                <div class="col s12 l10">
                    <div class="input-field">
                        <i class="material-icons prefix">search</i>
                        <input id="searchitem" type="text">
                        <label for="searchitem">Faite votre cherche ici</label>
                    </div>
                </div>
                <div class="s12 l3 offset-l1">
                    <a href="" class="btn center waves-effect waves-light">Valider</a>
                </div>
            </form>


        </div>

    </div>
    <div>
        <h2 class="flow-text center">Locataires</h2>
        <div class="row">
            <div class="col s12">

                <table class="centered responsive-table">
                    <thead>
                    <tr>
                        <th class="flow-text">Nom</th>
                        <th class="flow-text">Prénom</th>
                        <th class="flow-text">Contact</th>
                        <th class="flow-text"></th>
                    </tr>
                    </thead>

                    <tbody>
                    <tr>
                        <td>Kouiz</td>
                        <td>Mppopd</td>
                        <td>0000</td>
                        <td>
                            <a class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Modifier"
                               href="">
                                <i class="fi-xwsuxl-pen fi-2x"></i>
                            </a>
                            <a href="" class="tooltipped" data-position="bottom" data-delay="50"
                               data-tooltip="Supprimer">
                                <i class="fi-xnsuxl-trash-solid fi-2x"></i>
                            </a>
                        </td>
                    </tr>
                    <tr>
                        <td>Williams</td>
                        <td>Russels</td>
                        <td>0700000</td>
                        <td>
                            <a class="tooltipped" data-position="bottom" data-delay="50" data-tooltip="Modifier"
                               href="">
                                <i class="fi-xwsuxl-pen fi-2x"></i>
                            </a>
                            <a href="" class="tooltipped" data-position="bottom" data-delay="50"
                               data-tooltip="Supprimer">
                                <i class="fi-xnsuxl-trash-solid fi-2x"></i>
                            </a>
                        </td>
                    </tr>
                    <tr>
                        <td> </td>
                        <td> </td>
                        <td> </td>
                        <td> </td>
                    </tr>
                    <tr>

                    </tr>
                    </tbody>
                </table>

            </div>

        </div>
        <div>
            <a modal-trigger class="btn-floating right btn-large waves-effect modal-trigger  waves-light red"
               href="#propriomodal">
                <i class="material-icons">add</i>
            </a>
        </div>

        <div id="propriomodal" class="modal">
            <div class="model-content">
                <div class="container margin-top">
                    <h1 class="flow-text center ">Ajouter un nouveau locataires</h1>
                    <div class="row">
                        <form>
                            <div class="col s12 l6">
                                <div class="input-field">
                                    <input type="text" id="immo">
                                    <label for="immo">Entrer le nom</label>
                                </div>
                                <div class="input-field">
                                    <input type="text" id="nberoccupe">
                                    <label for="nberoccupe">Prénom</label>
                                </div>
                            </div>
                            <div class="col s12 l6">
                                <div class="input-field">
                                    <!-- <select>
                                        <option value="" disabled selected>Nom du propriétaire</option>
                                        <option value="1">Option 1</option>
                                        <option value="2">Option 2</option>
                                    </select> -->
                                    <input type="text" id="proprio">
                                    <label for="proprio">Contact</label>
                                </div>
                                <div class="input-field">
                                    <input type="text" id="nbretotal">
                                    <label for="nbretotal"> Adresse</label>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <a href="#!" class="red modal-close waves-effect waves-green btn">Fermer</a>
                <a href="#!" class="indigo whiet-text waves-effect waves-green btn">Valider</a>
            </div>

        </div>
    </div>
</section>